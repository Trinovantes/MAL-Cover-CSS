{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Generator",
            "request": "attach",
            "preLaunchTask": "Compose: generator",
            "restart": true,
            "port": 1001,
    
            "type": "node",
            "sourceMaps": true,
            "remoteRoot": "/app/",
            "skipFiles": [
                "<node_internals>/**"
            ],
        },
        {
            "name": "Debug Scraper",
            "request": "attach",
            "preLaunchTask": "Compose: scraper",
            "restart": true,
            "port": 1002,
    
            "type": "node",
            "sourceMaps": true,
            "remoteRoot": "/app/",
            "skipFiles": [
                "<node_internals>/**"
            ],
        },
        {
            "name": "Debug Web Server",
            "request": "attach",
            "preLaunchTask": "Compose: www",
            "restart": true,
            "port": 9229,
    
            "type": "node",
            "sourceMaps": true,
            "remoteRoot": "/app/",
            "skipFiles": [
                "<node_internals>/**"
            ],
        },
    ],
}

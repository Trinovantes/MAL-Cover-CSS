        "@nuxt/devalue": "^2.0.2",
        "@playwright/test": "^1.58.2",
        "@rollup/plugin-replace": "^6.0.3",
        "@sentry/cli": "^3.2.2",
        "@sentry/vue": "^10.40.0",
        "@stylistic/eslint-plugin": "^5.9.0",
        "@types/better-sqlite3": "^7.6.13",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.6",
        "@types/express-session": "^1.18.2",
        "@types/http-errors": "^2.0.5",

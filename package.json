{
    "type": "module",
    "private": true,
    "scripts": {
        "generateCss": "node --import=./build/preload.ts ./src/cron/generateCss.ts ./dist/web/client/generated",
        "scrapeUsers": "node --import=./build/preload.ts ./src/cron/scrapeUsers.ts",
        "startWeb": "   node    -r source-map-support/register ./dist/web/server/www.cjs",
        "watchWeb": "   nodemon -r source-map-support/register ./dist/web/server/www.cjs --watch ./dist/web/server",
        "devWebClient": "NODE_ENV=development webpack serve   --config build/webpack.config.web.client.ts",
        "devWebServer": "NODE_ENV=development webpack --watch --config build/webpack.config.web.server.ts",
        "buildWebClient": "                   webpack         --config build/webpack.config.web.client.ts && sentry-cli sourcemaps inject ./dist/web/client",
        "buildWebServer": "                   webpack         --config build/webpack.config.web.server.ts && sentry-cli sourcemaps inject ./dist/web/server",
        "dev": "concurrently 'pnpm devWebClient' 'pnpm devWebServer'",
        "build": "pnpm buildWebClient && pnpm buildWebServer",
        "test": "pnpm unit && playwright test",
        "unit": "vitest",
        "e2e": "playwright test",
        "lint": "vue-tsc && eslint",
        "analyze": "WEBPACK_ANALYZE=true NODE_ENV=production webpack --config build/webpack.config.web.client.ts"
    },
    "dependencies": {
        "@sentry/node": "^10.40.0",
        "@sinclair/typebox": "^0.34.48",
        "better-sqlite3": "^12.6.2",
        "date-fns": "^4.1.0",
        "dotenv": "^17.3.1",
        "drizzle-orm": "^0.45.1",
        "express": "^5.2.1",
        "pino": "^10.3.1",
        "pino-http": "^11.0.0",
        "pino-pretty": "^13.1.3",
        "source-map-support": "^0.5.21"
    },
    "devDependencies": {
        "@eslint/compat": "^2.0.2",

{
    "private": true,
    "scripts": {
        "startGenerator": "node    -r source-map-support/register ./dist/cron/generator.js ./dist/client/generated",
        "startScraper": "  node    -r source-map-support/register ./dist/cron/scraper.js",
        "startWeb": "      node    -r source-map-support/register ./dist/web/server/www.js",
        "watchWeb": "      nodemon -r source-map-support/register ./dist/web/server/www.js --watch dist/web/server",
        "devCron": "     NODE_ENV=development webpack --watch --config build/webpack.config.cron.ts",
        "devWebClient": "NODE_ENV=development webpack serve   --config build/webpack.config.web.client.ts",
        "devWebServer": "NODE_ENV=development webpack --watch --config build/webpack.config.web.server.ts",
        "buildCron": "                        webpack         --config build/webpack.config.cron.ts       && sentry-cli sourcemaps inject ./dist/cron",
        "buildWebClient": "                   webpack         --config build/webpack.config.web.client.ts && sentry-cli sourcemaps inject ./dist/web/client",
        "buildWebServer": "                   webpack         --config build/webpack.config.web.server.ts && sentry-cli sourcemaps inject ./dist/web/server",
        "dev": "   concurrently 'yarn devCron'      'yarn devWeb'",
        "devWeb": "concurrently 'yarn devWebClient' 'yarn devWebServer'",
        "build": " concurrently 'yarn buildCron'    'yarn buildWeb'",
        "buildWeb": "yarn buildWebClient && yarn buildWebServer",
        "test": "yarn unit && playwright test",
        "unit": "vitest",
        "e2e": "playwright test",
        "lint": "vue-tsc --noemit && eslint",
        "analyze": "WEBPACK_ANALYZE=true NODE_ENV=production webpack --config build/webpack.config.web.client.ts",
        "postinstall": "patch-package"
    },
    "dependencies": {
        "better-sqlite3": "^11.0.0",
        "express": "^4.17.1",
        "patch-package": "^8.0.0",
        "source-map-support": "^0.5.19"
    },
    "devDependencies": {
        "@eslint/js": "^9.4.0",
        "@nuxt/devalue": "^2.0.2",
        "@originjs/vite-plugin-require-context": "^1.0.9",
        "@playwright/test": "^1.21.1",
        "@rollup/plugin-replace": "^5.0.5",
        "@sentry/cli": "^2.21.2",
        "@sentry/node": "^7.2.0",
        "@sentry/tracing": "^7.2.0",
        "@sentry/vue": "^7.2.0",
        "@sinclair/typebox": "^0.32.12",
        "@stylistic/eslint-plugin": "^2.1.0",
        "@types/better-sqlite3": "^7.6.5",
        "@types/cors": "^2.8.14",
        "@types/express": "^4.17.9",
        "@types/express-session": "^1.17.7",
        "@types/http-errors": "^2.0.1",
        "@types/morgan": "^1.9.2",
        "@types/node": "^20.6.2",
        "@types/supertest": "^6.0.2",
        "@types/webpack-bundle-analyzer": "^4.4.1",
        "@types/webpack-env": "^1.18.1",
        "@unhead/ssr": "^1.8.10",
        "@unhead/vue": "^1.8.10",
        "@vitejs/plugin-vue": "^5.0.3",
        "concurrently": "^8.2.1",
        "connect-redis": "^7.0.0",
        "copy-webpack-plugin": "^12.0.2",
        "cors": "^2.8.5",
        "css-loader": "^7.1.2",
        "date-fns": "^3.3.0",
        "dotenv": "^16.0.0",
        "drizzle-orm": "^0.31.2",
        "esbuild-loader": "^4.0.2",
        "eslint": "^9.4.0",
        "eslint-plugin-n": "^17.7.0",
        "eslint-plugin-vue": "^9.9.0",
        "express-session": "^1.17.1",
        "highlight.js": "^11.0.1",
        "html-webpack-plugin": "^5.3.1",
        "http-errors": "^2.0.0",
        "mini-css-extract-plugin": "^2.0.0",
        "morgan": "^1.10.0",
        "null-loader": "^4.0.1",
        "pinia": "^2.0.14",
        "pino": "^9.2.0",
        "pino-http": "^10.1.0",
        "pino-pretty": "^11.0.0",
        "quasar": "^2.0.0-beta.14",
        "quasar-unused-plugin": "^0.3.1",
        "redis": "^4.0.4",
        "responsive-loader": "^3.0.4",
        "sass": "^1.32.8",
        "sass-loader": "^14.0.0",
        "sharp": "^0.32.6",

{
    "private": true,
    "scripts": {
        "startGenerator": "node    -r source-map-support/register ./dist/cron/generator.js ./dist/generated",
        "startScraper": "  node    -r source-map-support/register ./dist/cron/scraper.js",
        "start": "         node    -r source-map-support/register ./dist/web/www.js",
        "watch": "         nodemon -r source-map-support/register ./dist/web/www.js",
        "devCron": "  NODE_ENV=development      webpack --watch --config build/webpack.config.cron.ts",
        "devWeb": "   NODE_ENV=development      webpack --watch --config build/webpack.config.web.client.ts --config build/webpack.config.web.server.ts",
        "buildCron": "NODE_ENV=production       webpack         --config build/webpack.config.cron.ts",
        "buildWebClient": "NODE_ENV=production  webpack         --config build/webpack.config.web.client.ts",
        "buildWebServer": "NODE_ENV=production  webpack         --config build/webpack.config.web.server.ts",
        "buildWeb": "yarn buildWebClient && yarn buildWebServer",
        "dev": "  concurrently 'yarn devCron'   'yarn devWeb'",
        "build": "concurrently 'yarn buildCron' 'yarn buildWeb'",
        "lint": "vue-tsc --noemit && eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
        "postinstall": "patch-package"
    },
    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/preset-typescript"
        ]
    },
    "dependencies": {
        "@nuxt/devalue": "^2.0.0",
        "@sentry/node": "^6.6.0",
        "@sentry/tracing": "^6.6.0",
        "@sentry/vue": "^6.6.0",
        "@vue/server-renderer": "^3.1.1",
        "ajv": "^8.6.0",
        "axios": "^0.27.2",
        "connect-redis": "^6.0.0",
        "dayjs": "^1.10.1",
        "dotenv": "^16.0.0",
        "express": "^4.17.1",
        "express-session": "^1.17.1",
        "highlight.js": "^11.0.1",
        "http-errors": "^2.0.0",
        "lodash-es": "^4.17.21",
        "morgan": "^1.10.0",
        "patch-package": "^6.4.7",
        "pinia": "^2.0.14",
        "quasar": "^2.0.0-beta.14",
        "redis": "^4.0.4",
        "source-map-support": "^0.5.19",
        "sqlite": "^4.0.23",
        "sqlite3": "^5.0.2",
        "vue": "^3",
        "vue-meta": "^3.0.0-alpha.8",
        "vue-router": "^4"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/preset-env": "^7.12.11",

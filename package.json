{
    "private": true,
    "scripts": {
        "buildCron": "     webpack --config build/webpack.config.cron.ts",
        "buildWeb": "      webpack --config build/webpack.config.web.ts",
        "watchWeb": "      webpack --config build/webpack.config.web.ts --watch",
        "buildWebClient": "webpack --config build/webpack.config.web.client.ts",
        "buildWebServer": "webpack --config build/webpack.config.web.server.ts",
        "debugGenerator": "node -r source-map-support/register --inspect-brk=0.0.0.0:9229 ./dist/cron/generator.js static/generated",
        "debugScraper": "  node -r source-map-support/register --inspect-brk=0.0.0.0:9229 ./dist/cron/scraper.js",
        "runGenerator": "  node -r source-map-support/register ./dist/cron/generator.js static/generated",
        "runScraper": "    node -r source-map-support/register ./dist/cron/scraper.js",
        "runWeb": "        node -r source-map-support/register ./dist/web-server/www.js",
        "build": "concurrently 'yarn buildWeb' 'yarn buildWebClient' 'yarn buildWebServer'",
        "dev": "  concurrently 'yarn watchWeb' 'nodemon -r source-map-support/register --inspect=0.0.0.0:9229 ./dist/web-server/www.js --watch ./dist/web-server'",
        "lint": "   eslint --ext .js,.ts,.vue .",
        "lintFix": "eslint --ext .js,.ts,.vue . --fix"
    },
    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/preset-typescript"
        ]
    },

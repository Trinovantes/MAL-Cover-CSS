{
    "private": true,
    "scripts": {
        "startGenerator": "node    -r source-map-support/register ./dist/cron/generator.js ./dist/generated",
        "startScraper": "  node    -r source-map-support/register ./dist/cron/scraper.js",
        "startApi": "      node    -r source-map-support/register ./dist/api/www.js",
        "watchApi": "      nodemon -r source-map-support/register ./dist/api/www.js",
        "devCron": "  cross-env NODE_ENV=development webpack --watch --config build/webpack.config.cron.ts",
        "devApi": "   cross-env NODE_ENV=development webpack --watch --config build/webpack.config.api.ts",
        "devWeb": "   cross-env NODE_ENV=development webpack serve   --config build/webpack.config.web.ts",
        "buildCron": "cross-env NODE_ENV=production  webpack         --config build/webpack.config.cron.ts",
        "buildApi": " cross-env NODE_ENV=production  webpack         --config build/webpack.config.api.ts",
        "buildWeb": " cross-env NODE_ENV=production  webpack         --config build/webpack.config.web.ts",
        "dev": "  concurrently 'yarn devCron'   'yarn devApi'   'yarn devWeb'",
        "build": "concurrently 'yarn buildCron' 'yarn buildApi' 'yarn buildWeb'",
        "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore ."
    },
    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/preset-typescript"
        ]
    },
